{
  "title": "Longitude and Latitude Values (https://json-schema.org/learn/examples/geographical-location.schema.json)",
  "description": "A geographical coordinate.",
  "required": [ "latitude", "longitude" ],
  "type": "object",
  "properties": {
    "latitude": {
      "type": "number",
      "minimum": -90,
      "maximum": 90,
      "description": "(query:`Geo.Lat`)"
    },
    "longitude": {
      "type": "number",
      "minimum": -180,
      "maximum": 180,
      "description": "(query:`Geo.Lon`)"
    }
  }
}
