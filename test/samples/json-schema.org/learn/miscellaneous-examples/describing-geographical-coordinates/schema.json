{
  "title": "Longitude and Latitude Values (https://json-schema.org/learn/miscellaneous-examples.html)",
  "description": "A geographical coordinate.",
  "required": [
    "latitude",
    "longitude"
  ],
  "type": "object",
  "properties": {
    "latitude": {
      "description": "query:```lat```",
      "type": "number",
      "minimum": -90,
      "maximum": 90
    },
    "longitude": {
      "description": "query:```lon```",
      "type": "number",
      "minimum": -180,
      "maximum": 180
    }
  }
}